\begin_layout Plain Layout
Likformig fördelning
\end_layout

\end_inset


\end_layout

\begin_deeper
\begin_layout Itemize

\series bold
Täthetsfunktion
\series default
 för likformig fördelad slumpvariabel över support 
\begin_inset Formula $[a,b]$
\end_inset


\begin_inset Formula 
\[
f(x)=\frac{1}{b-a}\quad\text{för }a\leq x\leq b,
\]

\end_inset

och 
\begin_inset Formula $f(x)=0$
\end_inset

 annars.
\end_layout

\begin_layout Itemize
Väntevärde
\begin_inset Formula 
\[
\mathbb{E}X=\int x\cdot f(x)dx=\frac{1}{b-a}\int xdx=\frac{1}{b-a}
\]

\end_inset


\end_layout

\end_deeper
